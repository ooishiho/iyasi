<div class="container">
 <div class="row">
  <h1 class="col-lg-5"></h1>
 </div>
</div>
<h1>ユーザー一覧</h1>
  <table class="table">
  <thead>
    <tr>
      <th class="col-md-3">ユーザーID</th>
      <th class="col-md-3">ユーザーネーム</th>
      <th class="col-md-3">メールアドレス</th>
      <th class="col-md-3">ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= link_to user.name, admin_customer_path(user.id) %></td>
      <td><%= user.email %></td>
      <td><% if user.is_deleted == false %>
      有効
      <% else %>
      退会
      <% end %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>